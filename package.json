{
  "name": "@oclif/plugin-not-found",
  "description": "\"did you mean\" for oclif",
  "version": "1.0.5",
  "author": "Jeff Dickey @jdxcode",
  "oclif": {
    "hooks": {
      "command_not_found": "./lib"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "bugs": "https://github.com/oclif/plugin-not-found/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@oclif/command": "^1.4.4",
    "string-similarity": "^1.2.0"
  },
  "devDependencies": {
    "@oclif/config": "^1.3.60",
    "@oclif/dev-cli": "^1.4.2",
    "@oclif/errors": "^1.0.2",
    "@oclif/plugin-help": "^1.1.7",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.0.5",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/plugin-not-found",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "oclif/plugin-not-found",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test\"",
    "postpublish": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
